{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "black": "#262B44",
    "blue": "#4B95E9",
    "green": "#59C9A5",
    "orange": "#F07623",
    "red": "#D81E5B",
    "white": "#E0DEF4",
    "yellow": "#F3AE35"
  },
  "secondary_prompt": {
    "template": "<p:yellow,transparent>\ue0b6</><,p:yellow> > </><p:yellow,transparent>\ue0b0</> ",
    "foreground": "p:black",
    "background": "transparent"
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "{{ .PromptMark }}",
          "foreground": "#80ffea",
          "type": "iterm",
          "interactive": true
        },
        {
          "properties": {
            "style": "austin",
            "threshold": 500
          },
          "style": "powerline",
          "template": " <#fefefe>\udb81\udead</> {{ .FormattedMs }}</> ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#8800dd",
          "type": "executiontime"
        },
        {
          "properties": {
            "home_icon": "~",
            "mixed_threshold": 15,
            "style": "mixed"
          },
          "style": "powerline",
          "leading_diamond": "\ue0b6",
          "template": " \udb80\ude4b {{ path .Path .Location }} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:orange",
          "type": "path"
        },
        {
          "properties": {
            "branch_max_length": 40,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044  {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b0",
          "background": "p:green",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:black{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:white{{ end }}",
            "{{ if gt .Ahead 0 }}p:white{{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}#49416D{{ end }}",
            "{{ if gt .Behind 0 }}#7A306C{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf00d {{ .Meaning }}{{ else }}\uf00c{{ end }} ",
          "foreground": "p:white",
          "background": "p:green",
          "type": "exit",
          "background_templates": ["{{ if gt .Code 0 }}p:red{{ end }}"]
        }
      ]
    }
  ],
  "tooltips": [
    {
      "properties": {
        "display_default": true
      },
      "style": "diamond",
      "leading_diamond": "\ue0b6",
      "trailing_diamond": "\ue0b4",
      "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
      "foreground": "p:white",
      "background": "p:orange",
      "type": "aws",
      "tips": ["aws"]
    },
    {
      "style": "diamond",
      "leading_diamond": "\ue0b6",
      "trailing_diamond": "\ue0b4",
      "template": " \ufd03 {{ .Name }} ",
      "foreground": "p:white",
      "background": "p:blue",
      "type": "az",
      "tips": ["az"]
    }
  ],
  "version": 3,
  "final_space": true
}
