{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "black": "#262B44",
    "blue": "#4B95E9",
    "green": "#59C9A5",
    "darkgreen": "#06B800",
    "orange": "#F07623",
    "red": "#D81E5B",
    "white": "#E0DEF4",
    "yellow": "#F3AE35"
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "  󱞩 "
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "iterm_features": ["prompt_mark", "current_dir"],
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "diamond",
          "leading_diamond": " ",
          "trailing_diamond": "",
          "template": "{{ if gt .Code 0 }} {{ .Meaning }}{{ end }}",
          "foreground": "p:white",
          "background": "p:darkgreen",
          "background_templates": ["{{ if gt .Code 0 }}p:red{{ else }}p:darkgreen{{ end }}"],
          "type": "status"
        },

        {
          "properties": {
            "style": "austin",
            "threshold": 500
          },
          "style": "diamond",
          "leading_diamond": " ",
          "trailing_diamond": "",
          "template": "  {{ .FormattedMs }}</> ",
          "foreground": "#ffffff",
          "background": "#8800dd",
          "type": "executiontime"
        },

        {
          "properties": {
            "home_icon": "~",
            "mixed_threshold": 5,
            "style": "powerlevel",
            "max_width": 50
          },

          "style": "powerline",
          "leading_powerline_symbol": " ",
          "powerline_symbol": "",
          "template": " {{ path .Path .Location }} ",
          "foreground": "p:white",
          "background": "p:orange",
          "type": "path"
        },
        {
          "max_width": 80,
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "powerline_symbol": "",
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .BranchStatus }}{{ if .Working.Changed }}   {{ .Working.String }}{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }} ",
          "foreground": "p:black",
          "background": "p:green",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:black{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:white{{ end }}",
            "{{ if gt .Ahead 0 }}p:white{{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}#49416D{{ end }}",
            "{{ if gt .Behind 0 }}#7A306C{{ end }}"
          ]
        },

        {
          "min_width": 80,
          "properties": {
            "branch_max_length": 60,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "powerline_symbol": "",
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }}   {{ .Working.String }}{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }} ",
          "foreground": "p:black",
          "background": "p:green",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:black{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:white{{ end }}",
            "{{ if gt .Ahead 0 }}p:white{{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}#49416D{{ end }}",
            "{{ if gt .Behind 0 }}#7A306C{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true
          },
          "min_width": 120,
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": " {{ if gt .Code 0 }}{{ else }}{{ end }}",
          "foreground_templates": ["{{ if gt .Code 0 }}p:red{{ else }}p:darkgreen{{ end }}"],
          "background": "transparent",
          "type": "status"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "max_width": 120,
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "\n  {{ if gt .Code 0 }}{{ else }}{{ end }}",
          "foreground_templates": ["{{ if gt .Code 0 }}p:red{{ else }}p:darkgreen{{ end }}"],
          "background": "transparent",
          "type": "status"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true,
  "shell_integration": true,
  "enable_cursor_positioning": true
}
