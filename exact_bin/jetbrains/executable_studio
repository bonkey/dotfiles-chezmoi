#!/bin/bash
#Generated by JetBrains Toolbox 3.0.0.58470 at 2025-10-07T12:48:07

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/dbauke-mac/Applications/Android Studio.app/Contents/MacOS/studio" $wait --args "${intellij_args[@]}"